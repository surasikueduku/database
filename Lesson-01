select now() AS "Today's Date"

CREATE TABLE test
(
   testId INT PRIMARY KEY AUTO_INCREMENT,
   test_name VARCHAR(10) NOT NULL,
   test_date DATE NOT NULL,
   test_taker VARCHAR(15) NOT NULL
)

